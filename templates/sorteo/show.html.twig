{% extends 'base.html.twig' %}

{% block title %}Sorteo{% endblock %}

{% block body %}
    <h1>Sorteo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sorteo.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sorteo.name }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ sorteo.date ? sorteo.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CantidadNumeros</th>
                <td>{{ sorteo.cantidadNumeros }}</td>
            </tr>
            <tr>
                <th>Premio</th>
                <td>{{ sorteo.premio }}</td>
            </tr>
            <tr>
                <th>PrecioNumero</th>
                <td>{{ sorteo.precioNumero }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sorteo_index') }}">back to list</a>

    <a href="{{ path('app_sorteo_edit', {'id': sorteo.id}) }}">edit</a>

    {{ include('sorteo/_delete_form.html.twig') }}
{% endblock %}
